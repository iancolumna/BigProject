    package="christian.columna.com.bigproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".addCourse" />
        <activity android:name=".inputGrade" />
        <activity android:name=".input2ndGrade" />
        <activity android:name=".viewSemesters" />
        <activity android:name=".viewCourses" />
        <activity android:name=".addSemester" />
        <activity android:name=".viewCourseInfo" />
        <activity android:name=".Register"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>